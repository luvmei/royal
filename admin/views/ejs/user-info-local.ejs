  <% } else { %>
  <%- include('navBar-agent.ejs') %>
  <% } %>
  <%- include('modals.ejs', {user: user}) %>
  <div id="layoutSidenav">
    <% if (user.type === 9) { %>
    <%- include('sideBar-admin.ejs') %>
    <% } else { %>
    <%- include('sideBar-agent.ejs') %>
    <% } %>
    <div id="layoutSidenav_content">
      <main>
        <div class="container-fluid px-4">
          <h2 class="my-4">매장생성 회원 목록 및 요약 정보</h2>
          <div class="card mb-4">
            <div class="card-body">
              <table id="userInfoLocal" class="cell-border hover" style="width: 100%">
                <thead>
                  <tr>
                    <th>IDX</th>
                    <th>레벨</th>
                    <th>아이디</th>
                    <th>닉네임</th>
                    <th>에이전트 트리</th>
                    <th>보유금</th>
                    <th>포인트</th>
                    <th>총 입금</th>
                    <th>총 출금</th>
                    <th>총 입출금</th>
                    <th>총 베팅</th>
                    <th>총 당첨</th>
                    <th>총 당첨률</th>
                    <th>가입일자</th>
                    <th>최근 접속일시</th>
                    <th>최근 접속IP<i class="ps-1 pt-1 bi bi-question-circle-fill text-danger" data-bs-toggle="tooltip"
                        data-bs-placement="top" data-bs-custom-class="custom-tooltip"
                        data-bs-title="최근 접속IP가 다른 유저의 가입IP와 일치하면 빨간배경"></i></th>
                    <th>상태</th>
                    <th>플래티넘</th>
                    <th>골드</th>
                    <th>실버</th>
